<script>
var urllottery = global_path + 'index.php/activitycenter/lottery' ;
var urlrecords = global_path + 'index.php/activitycenter/records' ;
var user_code = getQueryString('code')||(RP.store.getItem('curr_user_info')?RP.store.getItem('curr_user_info').user_code:"");

var date=new Date();
var enddate="2016/4/5 23:59:59";//活动结束时间
var activity_code='A1603003';
var	params = {
		user_code:user_code,
		activity_code:activity_code
} ;
(function(){
	
	console.log(user_code);
	if(new Date(enddate).getTime()<=date.getTime()){
		  $('#end').show();
	}else{
		if(user_code==null||user_code==""){
			$('#nologin').show();
			$('body').bind("touchmove",function(event){ event.preventDefault(); });
		}else{
			commonAjax(urlrecords,params,function(data){
			   if(data.status&&data.status == '2000'){
				   if(data.data.times>=1){
						if(data.data.prizes.length>0){
							$('#lucky').show();
							$('.yyy-alert-mainimg img').attr('src',data.data.prizes[0].icon);
							$('#contenttitle').html(data.data.prizes[0].prize_title);
							$('#contentname').html(data.data.prizes[0].prize_name);
						}else{
							$('#pity').show();
						}
					}else{
						//可以抽奖
					}
			   }else if(data.status&&data.status == '80003'){
				   $('#start').show();
			   }else if(data.status&&data.status == '80005'){
				   $('#end').show();
			   }	
				
			})
		}
	}
})();

(function(){
	  
	 //flip-container_hover
	 $(".flip-container").click(function(){
	 	if(new Date(enddate).getTime()<=date.getTime()){
	 	    if(!$(this).hasClass('flip-container_hover')){
			  $('#end').show();
			}
		}else{
		 if(!$(this).hasClass('flip-container_hover')){
				 commonAjax(urllottery,params,function(data){
					 if(data.status == "80001"){
							$('#pity').show();
							$('body').bind("touchmove",function(event){ event.preventDefault(); });
					}else if(data.status == "80000"){
							$('#lucky').show();
							$('body').bind("touchmove",function(event){ event.preventDefault(); });						
							$('.yyy-alert-mainimg img').attr('src',data.data.icon);
							$('#contenttitle').html(data.data.prize_title);
							$('#contentname').html(data.data.prize_name);
						}else if(data.status == "80002"){
							$('#noprize').show();
							$('body').bind("touchmove",function(event){ event.preventDefault(); });
						}else if(data.status == "80003"){
							$('#pity').show();
							$('body').bind("touchmove",function(event){ event.preventDefault(); });
						}else if(data.status == "80005"){
							$('#end').show();
							$('body').bind("touchmove",function(event){ event.preventDefault(); });
						}else if(data.status == "80006"){
							$('#pity').show();
							$('body').bind("touchmove",function(event){ event.preventDefault(); });
						}else if(data.status == "80007"){
							$('#nochance').show();
							$('body').bind("touchmove",function(event){ event.preventDefault(); });
						}
					
				 });	
				 $(this).addClass("flip-container_hover");
		  }		
		}
		

	 });
   $('.closedialog').on('click',function(){
   	$('#nochance').hide();
   	$('#end').hide();
   	$('#lucky').hide();
   	$('#pity').hide();
   })
})();
</script>
