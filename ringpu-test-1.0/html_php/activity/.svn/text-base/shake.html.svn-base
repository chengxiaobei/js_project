    var enddate2="2017/2/8 23:59:59";//活动结束时间
    var startdate3="2016/2/12 00:00:00";//活动开始时间
    var enddate3="2016/7/12 23:59:59";//活动结束时间 */
	var activity_code='';
	if(date.getTime()>new Date(startdate1).getTime()&&date.getTime()<new Date(enddate1).getTime()){
		activity_code='A2016012919235';
	}
/* 	if(date.getTime()>new Date(startdate2).getTime()&&date.getTime()<new Date(enddate2).getTime()){
		activity_code='A2016012969678';
	}
	if(date.getTime()>new Date(startdate3).getTime()&&date.getTime()<new Date(enddate3).getTime()){
