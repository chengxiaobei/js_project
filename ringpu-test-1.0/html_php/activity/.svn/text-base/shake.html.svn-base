	(function(){	
	/* 	if(user_code==null||user_code==""){
			$('#nologin').show();
			$('body').bind("touchmove",function(event){ event.preventDefault(); });
			hasshakeflag = "true";
		} */
		if(new Date(startdate1).getTime()>date.getTime()){
			$('#todaystart').show();
			hasshakeflag = "true";
			//setTimeout(function(){$('#todaystart').hide()},3000)
			
		}
    })();
	
	(function(){
		window.onload = function() {
			var myShakeEvent = new Shake({
				threshold : 7
			});
			myShakeEvent.start();
			var aud = $("#myaudio").get(0);  
		    aud.pos = -1;    
		    aud.addEventListener('canplay', function(evt) {  
		        aud.play();  
		    });  
			window.addEventListener('shake', shakeEventDidOccur, false);
			//shakeEventDidOccur();
			function shakeEventDidOccur() {
				 if(aud.paused){  
	                    aud.play();  
	                }  
	                else{  
	                    aud.pause();             
	                }  
				if (hasshakeflag == "false") {
					commonAjax(urlshake,params,function(data){
						if(data){
							if(data.status == "80001"){
								$('#pity').show();
								$('body').bind("touchmove",function(event){ event.preventDefault(); });
								hasshakeflag = "true";
							}else if(data.status == "80000"){
								$('#bingo').show();
								$('body').bind("touchmove",function(event){ event.preventDefault(); });
								//$('#pricename').html(data.data.prize_name);
								$('.yyy-alert-price').attr('src',data.data.icon);
// 								$('#priceshow').show();
// 								$('#pricecontent').html(data.data.prize_title);
								hasshakeflag = "true";
							}else if(data.status == "80002"){
								$('#noprize').show();
								$('body').bind("touchmove",function(event){ event.preventDefault(); });
								hasshakeflag = "true";
							}else if(data.status == "80003"){
								$('#start').show();
								$('body').bind("touchmove",function(event){ event.preventDefault(); });
								hasshakeflag = "true";
							}else if(data.status == "80005"){
								$('#end').show();
								$('body').bind("touchmove",function(event){ event.preventDefault(); });
								hasshakeflag = "true";
							}else if(data.status == "80006"){
								$('#pity').show();
								$('body').bind("touchmove",function(event){ event.preventDefault(); });
								hasshakeflag = "true";
// 								$('#bingo').show();
// 								$('body').bind("touchmove",function(event){ event.preventDefault(); });
// 								$('#pricename').html(data.data.prize_name);
// 								$('.yyy-alert-price').attr('src',data.data.icon);
							}else if(data.status == "80007"){
								$('#nochance').show();
								$('body').bind("touchmove",function(event){ event.preventDefault(); });
								hasshakeflag = "true";
							}
				         } 

					});
				}

			}
		}
		$('#download').on('click',function(){
			window.location.href='../download/downloadApp.php';
		})
		$('.close').on('click',function(){
			$('#pity').hide();
			$('#bingo').hide();
			$('#noprize').hide();
			$('#start').hide();
			$('#end').hide();
			$('#nochance').hide();
			$('body').unbind("touchmove");
			hasshakeflag = "false";
		})
		$(window).unload(function(){
			 window.removeEventListener('shake', shakeEventDidOccur);			  
		});
    })();	
	</script>
