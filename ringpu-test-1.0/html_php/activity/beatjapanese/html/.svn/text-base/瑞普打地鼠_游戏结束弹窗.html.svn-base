<title>瑞普打地鼠_游戏结束弹窗</title>
<link type="text/css" rel="stylesheet" href="../css/rp_style.css">
</head>
<!--游戏前body的类为pre-game-bg,游戏中body的类为game-bg-->
<body class="nor-body game-bg">
  <!--遮罩-->
  <div class="mask"></div>
  <!--游戏结束弹窗-->
  <div class="gameover-alert-con">
  	<img class="gameover-alert-bg-img" src="../images/gameover_alert_bg.png">
  	<!--内容容器-->
  	<div class="content-div">
  		<p class="tip-p">您还需要再努把力，奖品离您越来越近了哦~</p>
  		<p class="nor-p">本次成绩：100</p>
  		<p class="nor-p">活动排名：2</p>
  		<!--按钮-->
  		<div class="play-again-btn-div">
  			<img class="play-again-btn" src="../images/play_again_btn.png">
  		</div>
  	</div>
  </div>
  <!--倒计时模块,倒计时数字替换图片即可-->
  <!--<div class="countdown-con">
  	<img class="countdown-img" src="../images/countdown1.png">
  </div>-->
  <!--顶端数据模块-->
  <div class="top-data-con">
  	<!--数据-->
  	<div class="right data-div">
  		<p>分数：<span class="data-span">300分</span></p>
  	</div>
  	<!--数据-->
  	<div class="right data-div">
  		<p>倒计时：<span class="data-span">30秒</span></p>
  	</div>
  	<!--清浮动-->
  	<div class="clearf"></div>
  </div>
  <!--地鼠洞模块-->
  <div class="hole-con">
  	<table class="hole-tbl">
  		<tbody>
  			<tr>
  				<td>
  					<img class="people-img" src="../images/hongjun1.png">
  				</td>
  				<td>
  					<img class="people-img" src="../images/hongjun2.png">
  				</td>
  				<td>
  					<img class="people-img" src="../images/guizi1.png">
  				</td>
  			</tr>
  			<tr>
  				<td>
  					<img class="people-img" src="../images/hongjun1.png">
  				</td>
  				<td>
  					<img class="people-img" src="../images/hongjun1.png">
  				</td>
  				<td>
  					<img class="people-img" src="../images/hongjun2.png">
  				</td>
  			</tr>
  			<tr>
  				<td>
  					<img class="people-img" src="../images/guizi1.png">
  				</td>
  				<td>
  					<img class="people-img" src="../images/guizi2.png">
  				</td>
  				<td>
  					<img class="people-img" src="../images/hongjun1.png">
  				</td>
  			</tr>
  		</tbody>
  	</table>
  </div>
  <!--活动说明模块-->
  <!--<div class="rule-con">
  	<p class="nor-p">活动起止日期</p>
  	<p class="nor-p">2016年9月28日0：00-2016年10月12日23：59</p>
  </div>-->
