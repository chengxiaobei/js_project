<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
<meta name="apple-mobile-web-app-capable" content="yes">
<title>选择支付方式</title>
<link type="text/css" rel="stylesheet" href="../../../css/rp_style2.css">
<script type="text/javascript" src="../../../less/less.js"></script>
</head>
<body>
  <div class="wrap">
    <header>
      <a class="back-btn" href=""></a>
      <h2>选择支付方式</h2>
    </header>
    <section class="main-section">
      <!--订单提交成功-->
      <div class="chpay-booksuccess">
        <!--图标-->
        <div class="chpay-bs-logo"></div>
        <!--订单号-->
        <div class="chpay-bs">
          <div class="chpay-success">订单提交成功!</div>
          <div class="chpay-bs-no">订单号：123123123123</div>
        </div>
      </div>
      <div class="chpay-box">
        <ul>
          <!--选中是 chpay-checked   没选中是 chpay-checked-->
          <li class="chpay-logo-zfb"><div class="chpay-check">支付宝</div></li>
          <li class="chpay-logo-wx"><div class="chpay-check">微信</div></li>
          <li class="chpay-logo-yl"><div class="chpay-check">银联</div></li>
          <li class="chpay-logo-xxzf"><div class="chpay-checked">线下支付</div></li>
        </ul>
      </div>
      <div class="center-total">
        <div class="cen-tot-price left">
          总计：<span>98</span>元
        </div>
        <div class="cen-tot-btn right"><a href="">提交订单</a></div>
        <div class="clearf"></div>
      </div>
    </section>
  </div>
</body>
</html>




