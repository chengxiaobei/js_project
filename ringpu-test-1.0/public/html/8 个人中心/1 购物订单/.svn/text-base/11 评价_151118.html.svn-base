<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
<meta name="apple-mobile-web-app-capable" content="yes">
<title>评价</title>
<link type="text/css" rel="stylesheet" href="../../../css/rp_style2.css">
<script type="text/javascript" src="../../../less/less.js"></script>
</head>
<body>
  <div class="wrap">
    <header>
      <a class="back-btn" href=""></a>
      <h2>评价</h2>
    </header>
    <section class="main-section">
      <!--物品1-->
      <div class="eval-pro-box">
      	<div class="eval-pro-praise">
      	  <!--商品图片-->
      	  <div class="eval-pro-img left">
      	  	<img src="../../../images/sc_pro_img2.jpg">
      	  </div>
      	  <!--商品评价-->
	  	  <div class="eval-pro-np left">
    	    <div class="eval-pro-name">安立消</div>
	  	    <ul class="eval-pro-ping">
	  	      <!--选中是蓝色 eval-blue ，没选中是灰色 eval-grey-->
	  	   	  <li class="eval-blue">好评</li>
	  	  	  <li class="eval-grey">中评</li>
	  	  	  <li class="eval-grey">差评</li>
	  	  	  <div class="clearf"></div>
	  	    </ul>
      	    <div class="clearf"></div>
	      </div>
	      <div class="clearf"></div>
      	</div>
      	<!--评论框-->
      	<div class="eval-pro-write">
      	  <textarea placeholder="写点什么吧"></textarea>
      	</div>
      	<!--图片-->
      	<div class="eval-pro-plimg">
      	  <ul>
      	  	<li><img src="../../../images/sc_pro_img2.jpg"></li>
      	  	<li><img src="../../../images/sc_pro_img2.jpg"></li>
      	  	<li><img src="../../../images/sc_pro_img2.jpg"></li>
      	  	<li class="eval-pro-addimg">
                    <a href=""><img src="../../../images/eval-addimg.png"></a>
                    <input type="file">
                  </li>
      	  	<div class="clearf"></div>
      	  </ul>
      	</div>
      </div>
      <!--物品2-->
      <div class="eval-pro-box">
      	<div class="eval-pro-praise">
      	  <!--商品图片-->
      	  <div class="eval-pro-img left">
      	  	<img src="../../../images/sc_pro_img2.jpg">
      	  </div>
      	  <!--商品评价-->
	  	  <div class="eval-pro-np left">
    	    <div class="eval-pro-name">安立消</div>
	  	    <ul class="eval-pro-ping">
	  	      <!--选中是蓝色 eval-blue ，没选中是灰色 eval-grey-->
	  	   	  <li class="eval-blue">好评</li>
	  	  	  <li class="eval-grey">中评</li>
	  	  	  <li class="eval-grey">差评</li>
	  	  	  <div class="clearf"></div>
	  	    </ul>
      	    <div class="clearf"></div>
	      </div>
	      <div class="clearf"></div>
      	</div>
      	<!--评论框-->
      	<div class="eval-pro-write">
      	  <textarea placeholder="写点什么吧"></textarea>
      	</div>
      	<!--图片-->
      	<div class="eval-pro-plimg">
      	  <ul>
      	  	<li><img src="../../../images/sc_pro_img2.jpg"></li>
      	  	<li><img src="../../../images/sc_pro_img2.jpg"></li>
      	  	<li><img src="../../../images/sc_pro_img2.jpg"></li>
      	  	<li><a href=""><img src="../../../images/eval-addimg.png"></a></li>
      	  	<div class="clearf"></div>
      	  </ul>
      	</div>
      </div>
      <!--服务评分--> 
      <div class="eval-pro-score">
      	<div class="eval-pro-ser">服务评分</div>
      	<div class="eval-pro-speed">
      	  <ul>
      	  	<li>
      	  	  <div class="eval-pro-sp left">商品包装</div>
      	  	  <ul class="right">
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-g"><a href=""></a></li>
      	  	  	<div class="clearf"></div>
      	  	  </ul>
      	  	  <div class="clearf"></div>
      	  	</li>
      	  	<li>
      	  	  <div class="eval-pro-sp left">物流速度</div>
      	  	  <ul class="right">
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-y"><a href=""></a></li>
      	  	  	<li class="eval-star-g"><a href=""></a></li>
      	  	  	<div class="clearf"></div>
      	  	  </ul>
      	  	  <div class="clearf"></div>
      	  	</li>
      	  </ul>
      	</div>
      </div>
      <!--发表评价-->
	  <div class="center-total">
	    <!--选中 eval-checked   没选中 eval-check-->
	    <div class="eval-anon eval-checked left">匿名评价</div>
	  	<div class="order-xp-shou-btn right"><a href="">发表评价</a></div>
	  	<div class="clearf"></div>	    
	  </div>
    </section>
  </div>
</body>
</html>




