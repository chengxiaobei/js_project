<title>更换手机号1</title>
<link type="text/css" rel="stylesheet" href="../../css/rp_style2.css">
<script type="text/javascript" src="../../less/less.js"></script>
</head>
<body>
  <div class="wrap">
    <header>
      <a class="back-btn" href=""></a>
      <h2>更换手机号</h2>
    </header>
    <section class="main-section">
      <!--请输入您注册的手机号-->
      <div class="ghsjh-regphone">
        <div class="ghsjh-tit">请输入您注册的手机号</div>
        <div class="ghsjh-input">
          <input placeholder="请输入11位手机号">
        </div>
      </div>
      <!--请输入您想要绑定的手机号-->
      <div class="ghsjh-pound">
        <div class="ghsjh-tit">请输入您想要绑定的手机号</div>
        <div class="ghsjh-input">
          <input placeholder="请输入11位手机号">
        </div>
      </div>
      <!--下一步按钮-->
      <div class="ghsjh-btn"><a href="">下一步</a></div>
    </section>
  </div>
