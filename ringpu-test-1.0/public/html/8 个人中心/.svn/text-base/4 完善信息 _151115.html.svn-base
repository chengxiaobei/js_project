<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
<meta name="apple-mobile-web-app-capable" content="yes">
<title>完善信息</title>
<link type="text/css" rel="stylesheet" href="../../css/rp_style2.css">
<script type="text/javascript" src="../../less/less.js"></script>
</head>
<body>
  <div class="wrap">
    <header>
      <a class="close-btn" href=""></a>
      <h2>完善信息</h2>
    </header>
    <section class="main-section">
      <!--头像-->
      <div class="wsxx-headpor">
        <img src="../../images/wsxx-headpor.png">
        <input type="file">
        <div class="wsxx-upload-head">上传头像</div>
      </div>
      <div class="reg-box">
        <!--用户名-->
        <div class="login-phone">
          <div class="login-sjmm left">昵称</div>
          <div class="login-input left">
            <input placeholder="4-10位数字、字母或中文" id="nickname" maxLength="10">
          </div>
          <div class="clearf"></div>
        </div>
        <!--用户名-->
        <div class="login-phone">
          <div class="login-sjmm left">姓名</div>
          <div class="login-input left">
            <input placeholder="请输入您的真实姓名" id="username">
          </div>
          <div class="clearf"></div>
        </div>
        <!--地区-->
        <div class="login-phone">
          <div class="login-sjmm left">地区</div>
          <div class="wsxx-area left" id="choose_area">
            <a class="wsxx-choose" href="javascript:void(0);">请选择</a>
          </div>
          <div class="clearf"></div>
        </div>
        <div class="login-phone">
          <div class="login-sjmm left">养殖厂名</div>
          <div class="login-input left">
            <input placeholder="请输入养殖厂单位名称" id="client_unit">
          </div>
          <div class="clearf"></div>
        </div>
        <div class="login-phone">
          <div class="login-sjmm left">全场养殖量</div>
          <div class="login-input left">
            <input placeholder="请输入全场养殖量范围或者精确数量" id="all_nums">
          </div>
          <div class="clearf"></div>
        </div>
        <div class="login-phone">
          <div class="login-sjmm left">品种</div>
          <div class="login-input left">
            <input placeholder="请输入养殖动物的品种" id="breed">
          </div>
          <div class="clearf"></div>
        </div>
        <div class="login-phone">
          <div class="login-sjmm left">级别</div>
          <div class="login-input left">
            <input placeholder="请输入养殖动物的品种级别" id="generation">
          </div>
          <div class="clearf"></div>
        </div>
        <!--邮箱-->
        <div class="login-password">
          <div class="login-sjmm left">邮箱</div>
          <div class="login-input left">
            <input placeholder="请输入常用邮箱" id="email">
          </div>
          <div class="clearf"></div>
        </div>
      </div>
      <a class="center-btn login-finbtn" href="">完成</a>
    </section>
  </div>
</body>
</html>




