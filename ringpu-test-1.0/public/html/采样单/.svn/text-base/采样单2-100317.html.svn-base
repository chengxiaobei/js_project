<title>采样单2</title>
<link type="text/css" rel="stylesheet" href="../../css/public.css">
<link type="text/css" rel="stylesheet" href="../../css/rp_style3.css">
</head>
<body class="whiteBg">
  <div class="wrap">
    <header>
      <a class="back-btn"></a>
      <h2>完善采样单</h2>
    </header>
    <section class="main-section-animal">
      <!--注释和完善进度-->
        <div class="cyd-jianxi-jindu">
          <div class="cyd-jianxi-jindumain">
            <p>采样单完善度</p>
            <img src="../../images/jindu-two.png">
          </div>
        </div>
      <!--第一行-->
      <div class="edit-animal-maincon cyd-stype-two">
        <div class="edit-lineonemain">
            <div class="edit-lineone">
              <p class="left edit-lineone-left">拟检疫病</p>
              <div class="clearf"></div>
            </div>
          </div>
        <div class="edit-linethreemain edit-linethreemain-shisi">
            <!--拟检疫病里面的7个小块中的第一个-->
            <div class="edit-linethree">
              <p class="left edit-linetwo-shisi edit-linetwo-shisione">抗体</p>
              <div class="clearf"></div>
              <div class="edit-linethree-textarea edit-linethree-textatwo">
                <textarea placeholder="请输入详情"></textarea>
              </div>
              <p class="edit-textareatext">0/<span>100</span></p>
            </div>
            <!--拟检疫病里面的7个小块中的第二个-->
            <div class="edit-linethree">
              <p class="left edit-linetwo-shisi edit-linetwo-shisione">病原</p>
              <div class="clearf"></div>
              <div class="edit-linethree-textarea edit-linethree-textatwo">
                <textarea placeholder="请输入详情"></textarea>
              </div>
              <p class="edit-textareatext">0/<span>100</span></p>
            </div>
            <!--拟检疫病里面的7个小块中的第三个-->
            <div class="edit-linethree">
              <p class="left edit-linetwo-shisi edit-linetwo-shisione">细菌</p>
              <div class="clearf"></div>
              <div class="edit-linethree-textarea edit-linethree-textatwo">
                <textarea placeholder="请输入详情"></textarea>
              </div>
              <p class="edit-textareatext">0/<span>100</span></p>
            </div>
            <!--拟检疫病里面的7个小块中的第四个-->
            <div class="edit-linethree">
              <p class="left edit-linetwo-shisi edit-linetwo-shisitwo">病毒、基因测序</p>
              <div class="clearf"></div>
              <div class="edit-linethree-textarea edit-linethree-textatwo">
                <textarea placeholder="请输入详情"></textarea>
              </div>
              <p class="edit-textareatext">0/<span>100</span></p>
            </div>
          </div>
        </div>
        <!--按钮-->
        <a href="">下一步</a>
    </section>
  </div>
</body>
