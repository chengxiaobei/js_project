/*索要发票模块*/
.sc-syfp-con{
  width: 100%;
  margin-bottom: 12px;
  background: #fff;
  .border-top;
  .border-bottom;
  >div{
    padding: 0 (100% - @main-width) / 2;
    .border-box;
    &:not(:last-child){.border-bottom;}
  }
  .label-con{
    height: 39px;
    .label-div{
      height: 39px;
      p{
