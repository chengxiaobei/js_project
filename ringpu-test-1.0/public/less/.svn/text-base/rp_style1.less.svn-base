@import "_base.less";

/*
 *【5 商城】7 温馨提示.html、7 温馨提示2.html、7 温馨提示3.html
 */
/*支付结果模块*/
.sc-payresult-con{
  width: 100%;
  padding: 42px (100% - @main-width) / 2 0;
  text-align: center;
  .border-box;
  .phone-num{
  	color: @rpcolor;
  	text-decoration: underline;
  }
  .hlight-a{color: @rpcolor;}
  .pay-again-btn{
  	margin: 26px auto 0;
  	.btn-base(@width: 35%, @height: 32px, @line-height: 32px, @font-size: @base-fsize - 2, @color: @rpcolor, @background: transparent, @hover-bg: #dbdbdb, @hover-color: @rpcolor);
  	.border(@border-color: @rpcolor);
  	.border-radius;
  	.border-box;
  }
  img{
  	width: 31px;
  	height: auto;
  	margin-bottom: 22px;
  }
  h1{
  	margin-bottom: 20px;
    padding: 0 20%;
  	//font-size: @base-fsize + 4;
    font-size: @base-fsize + 6;
  	line-height: @base-lheight;
  	font-weight: normal;
    .border-box;
  	&.pay-success{color: @rpcolor;}
  	&.pay-fail{color: #f24949;}
  }
  p.yzb-leftlogo{
    width: 118px;
    text-align:left;
    padding: 0;
    line-height: 32px;
    font-size:14px;
    background: url(../images/succes-yzb.png) no-repeat left center;
    background-size: 32px auto;
    margin: 0 auto;
    padding-left: 36px;
  }
  p{
    padding: 0 20%;
  	//font-size: @base-fsize - 2;
    font-size: @base-fsize;
  	color: @base-fcolor - #333;
  	line-height: 21px;
  	text-align: center;
    .border-box;
  }
  p + p{margin-top: 18px;}
}

/*
 *【5 商城】1 商城.html
 */
/*商城公共section*/
.sc-bar-section{padding-bottom: 40px;}
/*头部的搜索框模块*/
.sc-h-search-con{
  position: relative;
  width: 70%;
  height: 28px;
  margin: 8px auto;
  padding: 0 30px 0 35px;
  background: #fff;
  .border-radius(4px);
  .border-box;
  &.sc-index-h-search{
    width: 82%;
    margin: 8px 15% 8px 3%;
  }
  .search-btn{
  	position: absolute;
  	top: 0;
  	left: 0;
  	z-index: 500;
  	width: 35px;
  	height: 28px;
  	background: url(../images/search-img.png) no-repeat 9px center;
  	background-size: 17px auto;
  }
  .search-input{
  	width: 100%;
  	.input-base(@height: 28px, @padding: 0, @font-size: @base-fsize - 1, @p-font-size: @base-fsize - 1);
  	.border-box;
  }
  .clear-btn{
  	position: absolute;
  	top: 0;
  	right: 0;
  	z-index: 500;
  	width: 30px;
  	height: 28px;
  	padding-right: 4px;
  	background: url(../images/closed-img.png) no-repeat right center;
  	background-size: 14px auto;
  	background-origin: content-box;
  	.border-box;
  }
}
/*商品主容器*/
.sc-product-con{
  width: 100%;
  margin-bottom: 12px;
  background: #fff;
  .border-bottom;
  // 标题模块
  .title-div{
  	width: 100%;
  	height: 37px;
  	padding: 0 (100% - @main-width) / 2;
  	.border-bottom;
  	.border-box;
  	h1{
  	  padding-left: 24px;
  	  font-size: @base-fsize + 2;
  	  color: @base-fcolor;
  	  font-weight: normal;
  	  line-height: 37px;
  	  &.xlgj-h1{
  	  	background: url(../images/champion-img.png) no-repeat left center;
  	  	background-size: 15px auto;
  	  }
  	  &.cdj-h1{
  	  	background: url(../images/lowprice-img.png) no-repeat left center;
  	  	background-size: 14px auto;
  	  }
  	  &.rmfl-h1{
  	  	background: url(../images/fenlei-img.png) no-repeat left center;
  	  	background-size: 14px auto;
  	  }
  	}
  	// 更多按钮
  	.more-btn{
  	  display: block;
  	  height: 37px;
  	  font-size: @base-fsize - 2;
  	  color: @base-fcolor + #666;
  	  line-height: 37px;
  	}
  }
}
/*销量冠军*/
.sc-xlgj-ul{
  width: 100%;
  padding: 0 (100% - @main-width) / 2;
  .border-box;
  li{
    float: left;
    .border-bottom;
    .border-box;
    &:nth-child(3n+1){
      width: 34%;
      padding: 8px 2% 8px 0;
      .border-box;
    }
    &:nth-child(3n+2){
      width: 32%;
      padding: 8px 0;
      .border-box;
    }
    &:nth-child(3n){
      width: 34%;
      padding: 8px 0 8px 2%;
      .border-box;
    }
    &.no-borbottom{.border-bottom(0);}
    .pro-item-con{
      padding-bottom: 3px;
      .border;
      .border-box;
      // 图片容器
      .img-con{
        //position: relative;
        width: 100%;
        margin-bottom: 12px;
        padding: 50% 0;
        overflow: hidden;
        background-size: cover !important;
        img{
          width: 100%;
          height: auto;
        }
      }
      // 商品名称和规格
      .name-con{
        padding: 0 3px;
        .border-box;
        .name-div{
          width: 100%;
          margin-bottom: 2px;
          p{
            //font-size: @base-fsize - 2;
            font-size: @base-fsize;
            color: @base-fcolor;
            line-height: @base-lheight + .2;
            max-width: 100%;
            text-overflow: ellipsis;
            white-space: nowrap;
            overflow: hidden;
          }
        }
        .weight-div{
          width: 100%;
          margin-bottom: 2px;
          height:18px;
          p{
            //font-size: @base-fsize - 2;
            font-size: @base-fsize + 1;
            color: #fd0017;
            line-height: @base-lheight + .2;
          }
        }
      }
      // 标签
      .tag-con{
        height: 16px;
        padding: 0 3px;
        overflow: hidden;
        .border-box;
        span{
          padding: 2px;
          font-size: @base-fsize - 2;
          color: #fff;
          line-height: @base-lheight;
          background: @rpcolor;
          //transform: scale(0.8);
          //transform-origin: left;
          //&:not(:first-of-type){margin-left: -2px;}
          &:not(:first-of-type){margin-left: 3px;}
        }
      }
    }
  }
}
/*超低价商品列表*/
.sc-cdj-ul{
  width: 100%;
  padding: 0 (100% - @main-width) / 2;
  .border-box;
  li{
    position: relative;
    padding-left: 90px;
    .border-box;
  	&:not(:last-child){.border-bottom;}
  	.pro-img-con{
  	  // width: 28%;
      position: absolute;
      top: 0;
      left: 0;
      z-index: 1;
      width: 90px;
  	  // height: 100px;
      height: 100%;
  	  padding-top: 8px;
  	  text-align: left;
  	  .border-box;
  	  .pro-img-div{
  	  	display: inline-block;
  	  	width: 76px;
  	  	height: 84px;
  	  	text-align: center;
  	  	.border(@border-color: #cacaca);
  	  	.border-box;
  	  	img{
  	  	  width: auto;
  	  	  max-width: 100%;
  	  	  height: 82px;
  	  	}
  	  }
  	}
  	.pro-info-con{
  	  // width: 72%;
      width: 100%;
  	  height: 100px;
  	  .pro-info-div{
  	  	width: 70%;
  	  	height: 100px;
  	  	.name-con{
  	  	  margin-bottom: 6px;
  	  	  padding-top: 16px;
  	  	  .border-box;
  	  	  .name-div{
  	  	  	width: 80%;
  	  	  	p{
  	  	  	  //font-size: @base-fsize - 1;
              font-size: @base-fsize;
  	  	  	  color: @base-fcolor;
  	  	  	  line-height: @base-lheight;
  	  	  	}
  	  	  }
  	  	  .weight-div{
  	  	  	width: 50%;
  	  	  	p{ 
  	  	  	  position: relative;
  	  	  	  top: 3px;
              display: inline-block;
  	  	  	  font-size: @base-fsize - 2;
  	  	  	  color: #fff;
  	  	  	  line-height: @base-lheight;
  	  	  	  background: @rpcolor;
  	  	  	  padding: 2px 3px;
  	  	  	  //transform: scale(0.7);
  	  	  	}
  	  	  }
  	  	}
  	  	.explain-con{
  	  	  height: 42px;
  	  	  overflow: hidden;
  	  	  p{
  	  	  	font-size: @base-fsize - 2;
  	  	  	color: @base-fcolor + #666;
  	  	  	line-height: 14px;
  	  	  }
  	  	}
  	  }
  	  .pro-price-div{
  	  	width: 30%;
  	  	height: 100px;
  	  	padding-top: 39px;
  	  	padding-left: 14px;
  	  	.border-box;
  	  	.now-price{
  	  	  //padding-left: 4px;
  	  	  font-size: @base-fsize + 1;
  	  	  color: #fd0017;
  	  	  line-height: @base-lheight;
  	  	  margin-bottom: 1px;
  	  	  .border-box;
  	  	}
  	  	.origin-price{
  	  	  font-size: @base-fsize - 2;
  	  	  color: @base-fcolor + #666;
  	  	  line-height: @base-lheight;
  	  	  text-decoration: line-through;
  	  	  //transform: scale(0.85);
  	  	}
  	  }
  	}
  }
}
/*热门分类列表*/
// 热门分类列表商品大块儿
.sc-rmfl-ul-2{
  width: 100%;
  padding: 0 (100% - @main-width) / 2;
  .border-box;
  li{
    float: left;
    width: 50%;
    padding-top: 12px;
    padding-bottom: 8px;
    text-align: center;
    .border-right;
    .border-bottom;
    .border-box;
    &:nth-child(2n){.border-right(0);}
    p{
      margin-bottom: 8px;
      //padding-left: 6%;
      //font-size: @base-fsize - 1;
      font-size: @base-fsize;
      color: @base-fcolor;
      line-height: @base-lheight;
      //text-align: left;
      text-align: center;
      .border-box;
    }
    img{
      width: auto;
      max-width: 100%;
      height: 67px;
    }
  }
}
// 热门分类列表商品小块儿
.sc-rmfl-ul-4{
  width: 100%;
  padding: 0 (100% - @main-width) / 2;
  .border-box;
  li{
    float: left;
    //width: 25%;
    //padding-top: 10px;
    //padding-bottom: 8px;
    //text-align: center;
    .border-right;
    .border-bottom;
    .border-box;
    &:nth-child(4n){.border-right(0);}
    &.no-borbottom{.border-bottom(0);}
    width: 25%;
    width: calc(100% / 4);
    width: -moz-calc(100% / 4);
    width: -webkit-calc(100% / 4);
    padding-top: 4px;
    padding-bottom: 10px;
    text-align: center;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    p{
      //margin-bottom: 9px;
      //font-size: @base-fsize - 1;
      //font-size: @base-fsize;
      //color: @base-fcolor;
      //line-height: @base-lheight;
      //display: table-cell;
      vertical-align: middle;
      height: 2.4em;
      font-size: 14px;
      line-height: 1.2em;
      text-align: center;
    }
    img{
      width: auto;
      max-width: 100%;
      height: 50px;
    }
  }
}
/*底部总价条*/
.sc-total-price-bar{
  position: fixed;
  left: 0;
  bottom: 0;
  z-index: 601;
  width: 100%;
  height: 40px;
  padding-left: (100% - @main-width) / 2;
  background: #fff;
  .border-box;
  .price-div{
  	height: 40px;
  	p{
	    font-size: 12px;
	    color: #666;
	    line-height: 40px;
	    span{
	  	  font-size: 18px;
	  	  color: #fe0b00;
	    }
  	}
  }
  .price-div-ml{margin-left: 44px;}
  .nor-btn{
	  display: block;
    min-width: 105px;
    height: 40px;
    padding: 0 5px;
    background: #08a6f0;
    font-size: @base-fsize;
    color: #fff;
    text-align: center;
    line-height: 40px;
    .border-box;
    &:hover{background: #1b90cb;}
    &.nor-btn-dis{background: #d5d5d5;}
  }
}
/*购物车按钮模块*/
.sc-gwc-main-con{
  position: fixed;
  left: 0;
  bottom: 40px;
  z-index: 602;
  width: 100%;
  .gwc-btn-con{
    position: absolute;
    top: -47px;
    left: (100% - @main-width) / 2;
    z-index: 501;
    width: 40px;
    height: 40px;
    .gwc-btn{
      display: block;
      width: 40px;
      height: 40px;
      border-radius: 20px;
      background: rgba(31, 168, 236, .9) url(../images/rp_gwc_ico2.png) no-repeat center center;
      background-size: 21px auto;
      box-shadow: 1px 1px 2px 1px rgba(3, 4, 4, .4); 
    }
    .gwc-num{
	  position: absolute;
	  top: 0;
	  right: -2px;
	  z-index: 501;
	  width: 12px;
	  height: 12px;
	  border-radius: 6px;
	  background: #f00;
	  text-align: center;
	  line-height: 12px;
	  font-size: @base-fsize - 2;
	  color: #fff;
	  display: none;
	  span{display: none;}
    }
  }
  .gwc-btn-con-shrink{
    position: absolute;
    top: -20px;
    left: (100% - @main-width) / 2;
    z-index: 501;
    width: 40px;
    height: 40px;
    .gwc-btn{
	  display: block;
	  width: 40px;
	  height: 40px;
	  border: 1px solid #25b0f1;
	  border-radius: 20px;
	  background: rgba(255, 255, 255, .9) url(../images/rp_gwc_ico.png) no-repeat center center;
	  background-size: 21px auto;
	  box-shadow: 1px 1px 2px 1px rgba(3, 4, 4, .4);
    }
    .gwc-num{
	  position: absolute;
	  top: -2px;
	  right: -5px;
	  z-index: 501;
	  width: 16px;
	  height: 16px;
	  border-radius: 8px;
	  background: #f00;
	  text-align: center;
	  line-height: 16px;
	  font-size: 12px;
	  color: #fff;
	  display: block;
	  span{
		display: inline-block;
		transform: scale(0.8);
	  }
    }
  }
}
@media (max-width: 320px) {
  .sc-cdj-ul{
    li{
      .pro-info-con{
        .pro-info-div{width: 65%;}
        .pro-price-div{width: 35%;}
      }
    }
  }
}
@media (max-width: 320px){
  .sc-xlgj-ul{
    li{
      .pro-item-con{
        .name-con{
          .name-div{
            p{font-size: 12px;}
          }
        }
      }
    }
  }
}

/*
 *【5 商城】2 订单详情.html
 */
/*商城顶部有空隙的section*/
.sc-top-section{padding-top: 13px;}
/*收货地址模块*/
