  // 普通段落
  p{
    //font-size: @base-fsize - 1;
    font-size: @base-fsize + 3;
    color: @base-fcolor;
    //line-height: @base-lheight + .6;
    line-height: @base-lheight + .4;
    text-indent: 2em;
    text-align: left;
  }
  p + h2{margin-top: 13px;}
  h2{
    //font-size: @base-fsize + 1;
    font-size: @base-fsize + 2;
    color: @base-fcolor;
    line-height: @base-lheight + .4;
    text-align: left;
  }
  //插图
  img{
    width: 100% !important;
    height: auto !important;
  }
  //下面这个类是php中定义的，用来容纳后台传的内容
  .contentdetail{font-size: @base-fsize + 3;}
}
/*模块的标题*/
.model-title-con{
  width: 100%;
  padding: 10px (100% - @main-width) / 2;
  background: #e5e6e6;
  .border-box;
  p{
    font-size: @base-fsize + 1;
    color: @base-fcolor;
    line-height: @base-lheight;
    text-align: left;
  }
}
/*评论模块*/
.yzzx-comment-con{
  width: 100%;
  margin-bottom: 12px;
  padding: 0 (100% - @main-width) / 2;
  background: #fff;
  .border-top;
  .border-bottom;
  .border-box;
  // 评论列表
  .yzzx-comment-ul{
    li{
      position: relative;
      padding-left: 48px;
      .border-box;
      &:not(:last-child){.border-bottom;}
      &.no-comment-li{padding-left: 0;}
    }
    // 头像
    .head-img-con{
      position: absolute;
      top: 0;
      left: 0;
      z-index: 1;
      width: 48px;
      height: 100%;
      /*padding-top: 15px;*/
      padding-top: 12px;
      text-align: left;
      .border-box;
      img{
        width: 36px;
        height: 36px;
        border-radius: 36px;
      }
    }
    // 内容
    .info-div{
      min-height: 65px;
      padding: 9px 0;
      .border-box;
      // 左侧的名字和回复
      .name-reply-div{width: 72%;}
      // 日期
      .date-div{
        margin-bottom: 2px;
        p{
          font-size: @base-fsize - 2;
          color: @base-fcolor + #666;
          //line-height: 14px;
          line-height: 20px;
          text-align: right;
        }
      }
      // 名字
      .name-div{
        // margin-top: 5px;
        margin-top: 3px;
        margin-bottom: 6px;
        span{
          //font-size: @base-fsize - 1;
          font-size: @base-fsize + 3;
          color: @base-fcolor + #666;
          line-height: @base-lheight + .2;
          &:only-of-type{max-width: 100% !important;}
          &.uname-span{
            //max-width: 42%;
            max-width: 40%;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
            color: @rpcolor;
          }
        }
        span + span{margin-left: 2px;}
      }
      // 评论
      .comment-div{
        p{
          //font-size: @base-fsize - 1;
          font-size: @base-fsize + 3;
          color: @base-fcolor;
          line-height: @base-lheight + .2;
        }
      }
    }
    //暂无评论模块
    .no-comment-div{
      width: 100%;
      padding: 20px (100% - @main-width) / 2;
      background: #fff;
      .border-box;
      p{
        //font-size: @base-fsize - 2;
        font-size: @base-fsize;
        color: @base-fcolor + #666;
        line-height: @base-lheight + .2;
        text-align: center;
      }
    }
  }
  // 按钮模块
  .btn-con{
    width: 100%;
    padding: 0 (100% - @main-width) / 2 17px;
    .border-box;
    // 查看全部评论按钮
    .check-allcomm-btn{
      display: block;
      width: 36%;
      height: 24px;
      margin: 0 auto;
      font-size: @base-fsize;
      color: @rpcolor;
      text-align: center;
      line-height: 24px;
      .border(@border-color: @rpcolor);
      .border-radius(3px);
      &:hover{background: #dbdbdb;}
    }
  }
}
/*底部操作条*/
.yzzx-handle-bar{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 500;
  height: 40px;
  padding: 0 (100% - @main-width) / 2;
  background: #f5f5fa;
  .border-top(@border-color: #c5c5c5);
  .border-box;
  //容器
  .handle-div{
    width: 100%;
    height: 40px;
    /*padding-right: 90px;*/
    padding-right: 64px;
    .border-box;
  }
  // 输入框
  .input-div{
    width: 100%;
    height: 40px;
    padding-top: 8px;
    .comment-input{
      .input-base(@height: 25px, @background: #fff, @border: 1px solid #c5c5c5, @font-size: @base-fsize - 1, @p-font-size: @base-fsize - 1);
      .border-radius(3px);
      .border-box;
    }
  }
  // 按钮们
  .handle-btn-div{
    position: absolute;
    top: 0;
    bottom: 0;
    right: (100% - @main-width) / 2;
    z-index: 500;
    /*width: 90px;*/
    width: 70px;
    padding-left: 2px;
    /*margin-right: -4px;*/
    margin-right: -8px;
    .border-box;
    // 评论按钮
    .comment-btn{
      position: relative;
      display: block;
      // width: 33.33333333%;
      // width: ~"calc(100% / 3)";
      // width: ~"-moz-calc(100% / 3)";
      // width: ~"-webkit-calc(100% / 3)";
      width: 50%;
      width: ~"calc(100% / 2)";
      width: ~"-moz-calc(100% / 2)";
      width: ~"-webkit-calc(100% / 2)";
      height: 40px;
      background: url(../images/comment_ico.png) no-repeat center center;
      background-size: 18px auto;
      span{
        position: absolute;
        top: 6px;
        right: -1px;
        z-index: 500;
        display: block;
        min-width: 14px;
        background: #f00;
        padding: 2px 5px;
        font-size: @base-fsize - 2;
        color: #fff;
        line-height: @base-lheight;
        text-align: center;
        transform: scale(0.6);
        .border-radius(30px);
        &.comment-max-span{
          display: block;
          width: 14px;
          height: 10px;
          color: transparent;
          background: #f00 url(../images/comment_shenglue.png) no-repeat center center;
          background-size: 16px auto;
        }
      }
    }
    // 收藏按钮
    .collect-btn{
      display: block;
      // width: 33.33333333%;
      // width: ~"calc(100% / 3)";
      // width: ~"-moz-calc(100% / 3)";
      // width: ~"-webkit-calc(100% / 3)";
      width: 50%;
      width: ~"calc(100% / 2)";
      width: ~"-moz-calc(100% / 2)";
      width: ~"-webkit-calc(100% / 2)";
      height: 40px;
      background: url(../images/star_ico.png) no-repeat center 46%;
      background-size: 18px auto;
      &.collected-btn{
        background: url(../images/star_ico2.png) no-repeat center 46%;
        background-size: 18px auto;
      }
    }
    // 转发按钮
    .forward-btn{
      display: block;
      width: 33.33333333%;
      width: ~"calc(100% / 3)";
      width: ~"-moz-calc(100% / 3)";
      width: ~"-webkit-calc(100% / 3)";
      height: 40px;
      background: url(../images/forward_ico.png) no-repeat center center;
      background-size: 20px auto;
    }
  }
}
@media (max-width: 320px){
  .yzzx-comment-con{
    .yzzx-comment-ul{
      .info-div{
        .name-div{
          span{
            &.uname-span{max-width: 39%;}
          }
        }
      }
    }
  }
}

