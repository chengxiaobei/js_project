/*
 *公共弹窗
 */


.rp-tc{
  z-index: 700;
  position: fixed;
  top: 50%;
  left: 50%;
  //margin: -63px 0 0 -118px;
  margin: -103px 0 0 -118px;
  width: 236px; 
  //height: 126px;
  height: 166px;
  background-color: #fff;
  font-size: @base-fsize + 2;
  text-align: center;
  .border-radius(4px);
  .border-box;
  .rp-tc-tit{
    display: table-cell;
    vertical-align: middle;
    //height: 86px;
    height: 126px;
    .border-bottom;
    .border-box;
    p{
      padding: 0 1em;
      width: 236px;
      color: #666;
      .border-box;
    }
  }
  .rp-tc-btn{
    a{
      display: block;
      line-height: 40px;
      &:hover{background-color: #dbdbdb;}
    }
    //一个按钮
    .rp-tc-btn-one{
      border-radius: 0 0 4px 4px;
    }
    //两个按钮
    .rp-tc-btn-left{
      width: 50%;
      border-radius: 0 0 0 4px;
    }
    .rp-tc-btn-right{
      width: 50%;
      border-radius: 0 0 4px 0;
      .border-left;
      .border-box;
    }
    //建议用户点“右侧按钮”时的样式
    .rp-tc-blue{
      color: @rpcolor;
    }
  }
}
