//public内容
*{
  margin:0; 
  padding:0; 
  word-wrap:break-word;
/*  -webkit-touch-callout:none;
  -webkit-user-select:none;
  -khtml-user-select:none;
  -moz-user-select:none;
  -ms-user-select:none;
  user-select:none;*/
}
html{
  min-height:100%;
}
body{
  font-size:14px;
  font-weight:normal; 
  color:#333;
  background:#efeff4; 
  margin:0 auto;
  width:100%;
  -webkit-text-size-adjust:none;
  font-family:helvetica;
  min-height:100%;
  -webkit-tap-highlight-color:rgba(0,0,0,0);
}
img,input,select{
  vertical-align:middle;
}
img{
  border:0;
}
li{
  list-style-type:none;
}
table{
  border-collapse:collapse; 
  border-spacing:0;
}
th,td{
  padding:0;
}
input,button,select,textarea{
  outline:none;
  -webkit-appearance: none;
} 
a{
  color:#333; 
  outline:medium none; 
  text-decoration:none;
}
a:hover{color:#333;text-decoration:none;}

//变量定义
@main-width: 94%;
@base-fsize: 14px;
@base-fcolor: #333;
@base-lheight: 1em;
@rpcolor: #1fa8ec;
@input-height: 35px;
@input-pd: 8px;
@input-p-fsize: 14px;
@input-p-color: #999;

//基础样式定义
//浮动 、清除
.left{
  float:left !important; 
  display:inline;
}
.right{
  float:right !important; 
  display:inline;
}
.clearf{
  clear:both;
  line-height:0;
  height:0%;
  font:0px Arial; 
  overflow:hidden;
}

//边框相关
.border(@border-width: 1px, @border-style: solid, @border-color: #e2e2e2){
  border: @arguments;
}
.border-top(@border-width: 1px, @border-style: solid, @border-color: #e2e2e2){
  border-top: @arguments;
}
.border-bottom(@border-width: 1px, @border-style: solid, @border-color: #e2e2e2){
  border-bottom: @arguments;
}
.border-left(@border-width: 1px, @border-style: solid, @border-color: #e2e2e2){
  border-left: @arguments;
}
.border-right(@border-width: 1px, @border-style: solid, @border-color: #e2e2e2){
  border-right: @arguments;
}
//图片相关
.img-auto{
  width: 100%;
  height: auto;
}
//圆角相关
.border-radius(@border-radius: 3px){
  border-radius: @border-radius;
}
//盒子类型
.border-box{
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}
//按钮相关
.btn-base(@display: block, @width: @main-width, @height: 40px, @font-size: @base-fsize + 2, @color: #fff, @background: @rpcolor, @text-align: center, @line-height: 40px, @hover-bg: #1b90cb, @hover-color: #fff){
  display: @display;
  width: @width;
  height: @height;
  font-size: @font-size;
  color: @color;
  background: @background;
  text-align: @text-align;
  line-height: @line-height;
  &:hover{
  	background: @hover-bg;
  	color: @hover-color;
  }
}
//input相关
.input-base(@width: 100%, @height: @input-height, @padding: 0 @input-pd, @background: transparent, @font-size: @base-fsize, @color: @base-fcolor, @p-font-size: @input-p-fsize, @p-color: @input-p-color, @border: 0){
  width: @width;
  height: @height;
  padding: @padding;
  background: @background;
  font-size: @font-size;
  color: @color;
  border: @border;
  .border-box;
  &::-webkit-input-placeholder{
    font-size: @p-font-size;
    color: @p-color;
  }
  &::-moz-placeholder{
    font-size: @p-font-size;
    color: @p-color;
  }
}

//calc计算宽度
.width-calc(@width: 100%){
  width:-webkit-calc(@width);
  width:-moz-calc(@width);
  width:calc(@width);
}

