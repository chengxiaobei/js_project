@import "_base.less";
/*
 * 动物诊疗-多选_161125
 */
.animal-treat-choose{
	.choose-title{
		text-align: center;
		padding: 20px 0 26px;
		font-size: @base-fsize + 4;
	}
	.choose-li{
		ul{
			li{
				position: relative;
				float: left;
				margin: 0 2% 15px;
				min-width: 82% / 3;
				background-color: #EFEFF4;
				line-height: 30px;
				text-align: center;
				.border-radius;	
				&.curr{
					background-color: @rpcolor;
					color: #fff;
					.choose-curr{
						position: absolute;
						top: -8px;
						right: -8px;
						width: 16px;
						height: 16px;
						background: url(../images/animal_choose.png) no-repeat center center;
						background-size: 16px auto;
					}
				}		
			}
		}
	}
}
/*
 * 动物诊疗-填写成功_161125
 */
.animal-success-write{
	background: @rpcolor url(../images/animal_success.png) no-repeat 100% - (100% - @main-width) / 2 top;
	background-size: 76px auto;
	margin-bottom: 26px;
	/*.border-top(@border-color:#fff);*/
	.write-div{
		padding: 127px 0 65px;
		font-size: @base-fsize + 4;
		text-align: center;
		color: #fff;
		background: url(../images/animal_dui.png) no-repeat center 40px;
		background-size: 66px auto;
	}
}
.animal-btn-two{
	a{
		display: block;
		width: 83%;
		background-color: #fff;
		font-size: @base-fsize + 4;
		text-align: center;
		margin: 0 auto 10px;
		line-height: 44px;
		.border-radius;
		&.curr{
			background-color: @rpcolor;
			color: #fff;
		}
	}
}
/*送检单信息*/
.animal-sjd-info-con{
	width: @main-width;
	margin: 0 auto 20px;
	//送检单信息列表
	.sjd-info-ul{
		li{
			width: 100%;
			& + li{margin-top: 5px;}
			//label
			.label-div{
				//width: 27%;
        width: 31%;
				p{
					font-size: @base-fsize - 1;
					color: #666;
					line-height: 1.2em;
				}
			}
			//内容
			.content-div{
				//width: 73%;
        width: 69%;
				p{
					font-size: @base-fsize - 1;
					color: #333;
					line-height: 1.2em;
				}
			}
		}
	}
}
/*
 * 动物诊疗-填写检测申请-家畜1_161125
 */
.animal-exam-apply{
	width: 100%;
	padding: 12px 0;
	margin-bottom: 12px;
	background-color: #fff;
	text-align: center;
	.apply-box{
		display: inline-block;
	}
  ul{
  	li{
  		float: left;
  		position: relative;
  		padding: 0 2px;
  		max-width: 58px;
  		font-size: @base-fsize - 2;
  		text-align: center;
  		.apply-cirle{
  			text-align: center;
  			height: 20px;
  			color: #999;
  			padding: 0 2px;
  			display: inline-block;
  			margin: auto;
  			background: url(../images/strip_left_grey.png) no-repeat left center,url(../images/strip_right_grey.png) no-repeat right center;
  			background-size: auto 8px,auto 8px;
  			.cirle-just{
  				line-height: 20px;
  				width: 20px;
  				display: inline-block;
  				background-color: #F5F5F5;
  				.border-radius(20px);
  			}
			  .cirle-line{
			  	width: 52px;
			  	height: 2px;
			  	background-color: #f5f5f5;
			  	position: absolute;
			  	top: 9px;
			  	left: 65%;
			  	&.curr{
			  		background:#f5f5f5 url(../images/strip_line.png) no-repeat left center;
			  		background-size: 40px 2px;
			  	}
			  }
  		}
  		.apply-title{
  			color: #ccc;
  			margin-top: 6px;
  			text-align: center;
  		}
			&.curr{
				.apply-cirle{
					background: url(../images/strip_left_blue.png) no-repeat left center,url(../images/strip_right_blue.png) no-repeat right center;
  			  background-size: auto 8px,auto 8px;
  			  .cirle-just{
					  background-color: #1FA8EC;
					  color: #fff;
					}
				}
				.apply-title{
  			  color: #333;
  		  }
  		  .cirle-line{
  		  	background-color: @rpcolor;
  		  }
			}
			&:first-child{
  			.apply-cirle{
  				background: url(../images/strip_right_blue.png) no-repeat right center;
			    background-size: auto 8px;
			    .cirle-just{
					  background-color: #1FA8EC;
					  color: #fff;
					}
  			}
				.apply-title{
  			  color: #333;
  		  }
			}
			&:last-child{
  			.apply-cirle{
  				background: url(../images/strip_left_grey.png) no-repeat left center;
			    background-size: auto 8px;
  			}
			}
			&:last-child.curr{
				.apply-cirle{
  				background: url(../images/strip_left_blue.png) no-repeat left center;
			    background-size: auto 8px;
  			}
  			.apply-title{
  			  color: #333;
  		  }
			}
  	}
  }
}
.exam-five ul li{
	padding: 0 8px;
}
@media (min-width: 360px) {
	.animal-exam-apply ul li{
		padding: 0 5px;
		.apply-cirle .cirle-line{
			width: 60px;
			left: 65%;
		}
	}
}
@media (min-width: 400px) {
	.animal-exam-apply ul li{
		padding: 0 8px;
		.apply-cirle .cirle-line{
			width: 68px;
			left: 63%;
		}
	}
}
@media (min-width: 360px) {
	.exam-five ul li{
		padding: 0 12px;
		.apply-cirle .cirle-line{
			width: 60px;
			left: 65%;
		}
	}
}
@media (min-width: 400px) {
	.exam-five ul li{
		padding: 0 16px;
		.apply-cirle .cirle-line{
			width: 68px;
			left: 63%;
		}
	}
}
//内容
.animal-apply-con{
	background-color: #fff;
	font-size: @base-fsize - 1;
	margin-bottom: 56px;
	.border-top;
	ul{
		li{
			.border-bottom;
			>div{
				padding: 10px (100% - @main-width) / 2;
			}
			.conli-left{
				line-height: 26px;
				width: 36%;
				span{
					padding-right: 8px;
					background: url(../images/redxing-img.png) no-repeat right center;
					background-size: 5px auto;
				}
			}
			.conli-right{
				width: 64%;
				input{
					border: 0;
					width: 100%;
					line-height: 26px;
					&::-webkit-input-placeholder{
				    font-size: @base-fsize - 1;
				    color: #ccc;
				  }
				  &::-moz-placeholder{
				    font-size: @base-fsize - 1;
				    color: #ccc;
				  }
				}
				.div-input{
				  line-height: 20px;
				  padding: 3px 0;
				  &.curr{
				  	font-size: @base-fsize - 1;
				    color: #ccc;
				  }
				}
				.con-check{
					padding-left: 22px;
					background: url(../images/animal_check.png) no-repeat left center;
					background-size: 14px auto;
					line-height: 26px;
					width: 20%;
					&.curr{
						background: url(../images/animal_checked.png) no-repeat left center;
					  background-size: 14px auto;  
					}
				}
			}
			.right-edit{
				color: @rpcolor;
				text-align: right;
				line-height: 26px;
			}
			.grey-input{
				width: 64%;
				input{
					border: 0;
					width: 82%;
					background-color: #EFEFF4;
					line-height: 26px;
					padding: 0 6px;
					.border-box;
					&::-webkit-input-placeholder{
				    font-size: @base-fsize - 1;
				  }
				  &::-moz-placeholder{
				    font-size: @base-fsize - 1;
				  }
				}
				>span{
					display: inline-block;
          width: 18%;
          padding-left: 8px;
          line-height: 26px;
          .border-box;
				}
			}
		}
	}
	//增加检测样品
	.con-addsample{
		line-height: 44px;
		padding: 0 0 0 (100% - @main-width) / 2 ;
		.border-bottom;
		.sample-add{
			background: url(../images/sample_add.png) no-repeat center center;
			background-size: 18px auto;
			width: 44px;
			height: 44px;
		}
	}

	.con-sample{
		.sample-title{
			padding: 0 (100% - @main-width) / 2;
			line-height: 44px;
			color: @rpcolor;
			background: url(../images/sample_shang.png) no-repeat 100% - (100% - @main-width) / 2 center;
			background-size: 16px auto;
			.border-bottom;
			.title-del{
				background: url(../images/sample_del.png) no-repeat left center;
				background-size: 15px auto;
				width: 30px;
				height: 42px;
				display: none;
			}
			&.curr{
				color: #333;
				background: url(../images/sample_xia.png) no-repeat 100% - (100% - @main-width) / 2 center;
			  background-size: 16px auto;
				.title-del{
					display: inline-block;
				}
			}
		}
	}
	.text-area{
		position: relative;
		padding: 12px (100% - @main-width) / 2;
		background-color: #EFEFF4;
		.border-top;
		textarea{
			padding: 10px 10px 20px;
			height: 100px;
			width: 100%;
			border: 0;
			resize:none;
			font-size: @base-fsize - 2;
			.border-box;
		}
		.text-no{
			position: absolute;
			bottom: 14px;
			right: 16px;
			color: #999;
		}
	}
	.con-step5{
		.conli-left{
			width: 86%;
		}
		.conli-right{
			width: 14%;
		}
	}
}

//检测样品费用预估
.animal-apply-fee{
	float:right;
	height:80px;
	margin-top:-40px;
	margin-right:14px;
	span{
		color:red;
	}
}

//下一步
.animal-exam-next{
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	font-size: @base-fsize + 4;
	line-height: 44px;
	background-color: @rpcolor;
	text-align: center;
	color: #fff;
	a{
		display: block;
		color: #fff;
	}
}
/*
 * 个人中心-个人中心－检测报告文件_161130
 */
.center-report-file{
	margin-top: 56px;
	.file-each{
		padding: 12px (100% - @main-width) / 2;
		margin-bottom: 12px;
		font-size: @base-fsize - 1;
		line-height: 20px;
		background: #fff url(../images/center_right.png) no-repeat 100% - (100% - @main-width) / 2;
		background-size: 10px auto;
		.border-top;
		.border-bottom;
	}
}









