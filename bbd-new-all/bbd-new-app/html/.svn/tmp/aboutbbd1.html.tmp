        console.info($(this));
        var span_name = $(this).attr('id').split('span')[0];
        $(this).closest('.bbd-head').next().children().each(function(){
          var div_name = $(this).attr('id').split('div')[0];
          if(span_name == div_name){
            $("#"+span_name+"span").show();
            $("#"+span_name+"span").siblings().hide();
            $("#"+div_name+"div").show();
            $('#'+div_name+"div").siblings().hide();
          }
        });
    });

