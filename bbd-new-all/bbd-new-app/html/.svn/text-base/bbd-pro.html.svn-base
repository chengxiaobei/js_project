  <script type="text/javascript">
    $(document).ready(function(){
      console.info(window.location.href.split("p=")[1]);
      var href_name = window.location.href.split("p=")[1];
       $("#"+href_name+"span").show();
       $("#"+href_name+"span").siblings().hide();
       $("#"+href_name+"div").show();
       $('#'+href_name+"div").siblings().hide();

      
      //显示导航
      $('#secondchoose').click(function(){
        // $('.bbd-head').find('span').css('z-index','2');
        // console.info($('.bbd-head').find('span').css('z-index'));
        $('#Contdiv').children().each(function(){
            // console.info($(this));
            // console.info($(this).css('display'));
            // console.info($(this).css('z-index'));
            if($(this).css('display')=='block'){
              console.info($(this).attr('id'));
              var cname = $(this).attr('id').split('div')[0];
              $('#'+cname+'span').hide();
            }
        })
        $('.bbd-head').find('span').toggle();
      })
      //点击头部导航栏
      //console.info($('.bbd-head').find('span'));

      $('.bbd-head').find('span').click(function(){
        // console.info($(this));
        var span_name = $(this).attr('id').split('span')[0];
        $(this).closest('.bbd-head').next().children().each(function(){
          var div_name = $(this).attr('id').split('div')[0];
          if(span_name == div_name){
            $("#"+span_name+"span").show();
            $("#"+span_name+"span").siblings().hide();
            $("#"+div_name+"div").show();
            $('#'+div_name+"div").siblings().hide();
            window.scrollTo( 0, 0 );
          }
        });
      });

      //头部导航栏滑动定住
      $(window).scroll(function() {
        if($(window).scrollTop()> 36){
          $(".bbd-head").addClass('twoheader-fixed');
        }else{
          $(".bbd-head").removeClass('twoheader-fixed');
        }
      });
      //返回顶部
      $(".returntop").hide();
      $(window).scroll(function() {
        if($(window).scrollTop()> $(window).height()){
          $(".returntop").show();
        }else{
          $(".returntop").hide();
        }
      }); 
    })
  </script>
