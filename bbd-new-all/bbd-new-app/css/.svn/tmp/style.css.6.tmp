/*头部*/
.index-header{width:100%;background:black;}
.header-main{width:94%;height:36px;margin:0 auto;}
.header-main .header-main-righttext{width:72%; display:inline-block;}
.header-main img.header-leftlogo{width:80px;height:auto;margin-top:6px;}
.header-main ul{display:block;text-align:right;}
.header-main ul li{display:inline-block;color:#fff;font-size:12px;height:36px;padding:0 1%;line-height:36px;}
.header-main ul li.header-checkedtext{background:#008cd6;}
/*大轮播*/
.index-lunbo{position:relative;width:100%;text-align: center;overflow: hidden;}
.index-lunbo .index-lunbomain{position: relative; /*left: 50%;*/height:164px;width: 10000px;/*margin-left: -5000px;*/ text-align: center;}
.index-lunbo .index-lunbomain img{width:auto;height:180px;}
.index-mengceng{position: absolute;bottom: 0;width: 100%;height: 70px;background: url(../images/zhezhao-img.png) repeat left top;z-index: 2;}
.index-mengceng .index-mengceng-main{position:relative;width:88%;height: 70px;margin:0 auto;color:#fff;text-align:left;}
.index-mengceng .index-mengceng-main h3{font-size:14px;padding-top:8px;}
.index-mengceng .index-mengceng-main h4{font-size:12px;padding-top:5px;}
.index-mengceng .index-mengceng-main p{font-size:12px;}
.index-mengceng .index-mengceng-main .index-lb-btn{position:absolute;bottom:15px;right:0;z-index: 2;}
.index-mengceng .index-mengceng-main .index-lb-btn a{display:inline-block;width:16px;height:4px;background:#dedede;}
.index-mengceng .index-mengceng-main .index-lb-btn a.lb-checked{background:#008cd6;}
/*首页第二行核心产品*/
.index-hx-linetwo{width:100%;height:38px;background:#f5f5f5;}
.index-hx-linetwo .index-hx{height:30px;line-height:30px;padding-top:4px;}
.index-hx-linetwo p{border-left:5px solid #d4d4d4;padding-left:10px;color:#999;}
.index-hx-bottom{width:100%; overflow: hidden;}
.index-hx-bottom .index-hx-bottommain{width:93%;/*width:1000px;*/}
.index-hx-bottom .index-hx-bottomone{/*width:31%;width:100px;*/height:170px;text-align:center;margin: 20px 0 0 2%;}
.index-hx-bottom .index-hx-bottomone img{width:65px; height:auto;}
.index-hx-bottom .index-hx-bottomone p.index-hx-toptext{color:#ff0000;font-size:12px;padding-top:20px;}
.index-hx-bottom .index-hx-bottomone p.index-hx-bottomtext{font-size:14px;}
.index-hx-bottom .index-hx-bottomone a{display:inline-block;font-size:12px; color:#fff; width: 70px;height: 20px;line-height: 20px;margin-top:5px;border-radius:20px;background:#70ceff;}
.huibeijing{height:15px;background:#f5f5f5;}
/*互联网*/
.index-hlw{width:100%;height:276px;text-align:center;}
.index-hlw img.hlw-img{width:130px;height:auto; margin: 10px auto 30px;}
.index-hlw .index-hlw-tab li{float:left;width: 33.3333%; width:calc(100%/3); -webkit-width:calc(100%/3);height: 100px;}
.index-hlw .index-hlw-tab .hlw-tab-main{width:80px;margin: 0 auto;}
.index-hlw .index-hlw-tab img{width:60px;height:auto;}
.index-hlw .index-hlw-tab p{padding-top:10px;}
/*联系我们*/
.index-lxus{width:100%;/*height:180px;*/padding-bottom:30px;background:#0f94cc;}
.index-lxus .index-lxus-main{width:94%;margin: 0 auto;}
.index-lxus .index-lxus-main ul{padding: 10px;}
.index-lxus .index-lxus-main ul li{float:left;width:48%;margin-right:2%; color:#fff;padding-top:20px;box-sizing: border-box;}
.index-lxus .index-lxus-main ul li img{padding:0 12px 0 2px;  width: 16px;height:auto;}
.index-lxus .index-lxus-main ul li p{font-size:13px;width:72%; width: calc(100% - 30px);width: -moz-calc(100% - 30px); width: -webkit-calc(100% - 30px);}
.index-lxus .index-lxus-main ul li.index-lxus-one{width:42%;}
.index-lxus .index-lxus-main ul li.index-lxus-two{width:54%;}
.footer-main{width:100%;height:40px;line-height:40px;text-align:center;color:#999;font-size:13px;}
@media (min-width: 374px){
.header-main ul li{padding:0 5px;}
 }
@media (max-width: 330px){
.header-main ul li{padding:0;}
 }
@media (max-width: 340px){
.index-lxus .index-lxus-main .index-lxus-only{width:92px;padding-right:0;}
.index-lxus .index-lxus-main ul li img{padding-right:5px;}
 }
@media (max-width: 395px){
.footer-main{font-size:12px;}
 }

/*C2B页面*/
