/* 弹窗与我联系 */
.alert1-input{position:fixed;top:0;bottom:0;left:0;right:0; width:100%;padding:34px 3%;background:#23a2e5;box-sizing: border-box;}
#alert-close{position:absolute;right:3%;top:16px;background:url(../images/close.png) no-repeat;display: inline-block;width: 24px;height: 10px;}
.alert1-input h2{font-weight: normal;color: #fff;font-size: 21px;margin-bottom: 11px;width: 89.9%;}
.alert1-input p{color: #fff;font-size: 13px;margin-bottom: 15px;width: 89.9%;}
.alert1-input input{width: 96.5%;height: 34px;margin-bottom: 15px;padding-left: 3%;font-size: 14px;border: 1px solid #9be3ff;border-radius:2px;}
.alert1-input textarea{width:96.5%;padding-left: 3%;font-size: 14px;border: 1px solid #9be3ff;border-radius:2px;resize: none;height: 94px;margin-bottom: 30px;}
.alert1-input h3{font-weight: normal;font-size: 12px;color: #fadda6;margin-bottom: 36px}
.alert1-contact{height: 40px;width:99.5%;background: #1df50c;border-radius: 3px;text-align: center;line-height: 40px;margin-bottom: 36px;margin-bottom: 56px;}
.alert1-contact a{color: #ffffff;,}
