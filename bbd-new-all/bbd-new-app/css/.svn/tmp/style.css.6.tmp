/* bbd1 */
.bbd1-body{width: 100%;box-sizing: border-box;padding: 32px 3%;}
.bbd1-body h2{font-size: 18px;margin-bottom: 34px;}
.bbd1-body .bbd1-list p{font-size:13px;margin-bottom: 37px;}
.bbd1-body span{font-size: 13px;color: #ff0000;display: inline-block;margin-bottom: 13px;}
.bbd1-body h3{font-size: 13px; margin: 13px 0;font-weight: normal;}
.bbd1-body p{font-size:13px;}
.bbd-head p{height:30px; width: 100%; background: #f5f5f5; font-size: 14px;text-align: center;line-height: 30px;color: #333;margin: 0;}
.bbd-head span{display: inline-block;height:26px;width: 100%;background:#333;opacity:0.9; color:#fff;line-height: 26px;text-align: center;font-size: 12px;margin-bottom:1px;}
.bbd-head{position: relative;}
.bbd-head.twoheader-fixed{position: fixed;left: 0;top: 0;bottom: 0;right: 0;}
.bbd-head a{display: inline-block;position: absolute;background: url(../images/secondchoose.png);height: 33px;width: 31px;top: 15px;right:3%;z-index:100;}
/* bbd2 */
.bbd2-body{width: 100%;padding:32px 3%;box-sizing: border-box;padding-bottom: 0px;}
.bbd2-body li h2{font-weight: normal;font-size:23px;color: #c5c5c5;margin-bottom: 26px;}
.bbd2-body li p{font-size: 13px;line-height: 20px;margin-bottom:40px;}
/* bb3 */
.bbd3-text{width: 100%;padding: 30px 3%; box-sizing: border-box;}
.bbd3-text h2{font-size: 18px;margin-bottom: 34px;}
.bbd3-text p{font-size: 13px;line-height: 21px;margin-bottom: 76px;}
.bbd3-text h3{font-size: 18px;}
.bbd3-list{width:94%;background:#dcdcdc;margin: 0 auto 36px;padding:15px 0;box-sizing: border-box;}
.bbd3-list li{position: relative;text-align:left; width:26%;background:#f5f5f5;margin:15px 3.6%;}
.bbd3-list img{width:100%;}
.bbd3-list  li span{display: block;font-size: 12px;width: 80%;margin: 0 auto; padding-top: 5px;}
.bbd3-list li p{font-size:12px;color: #999;width: 80%;margin: 0 auto;padding: 3px 0 15px 0; line-height: 12px;}
/* 商业案例航空 */
.bbd-head .syal-color{color: #FF0000;}
.head-img img{width: 100%;margin-top:-1px;}
.syal-body{width:100%;box-sizing: border-box;padding:27px 3%;}
.syal-body li h1{font-size: 18px;margin-bottom:38px;}
.syal-body li h2{font-size: 15px;font-weight: normal;color: #008cd6;margin-bottom: 6px;}
