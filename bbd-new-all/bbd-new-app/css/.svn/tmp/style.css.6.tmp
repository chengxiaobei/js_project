.ctwob-waiborder .ctwob-border-con{padding:0 20px;  margin: 35px auto 25px;}
.ctwob-waiborder .ctwob-border-con p{font-size:12px;text-align:left;line-height:20px;}
.ctwob-waiborder .ctwob-shhyx img{width:96%;}
/*B2B页面*/
.ctwob-lineone .ctwob-fourgn{text-align:center;margin-bottom:50px;}
.ctwob-lineone .ctwob-fourgn img{width:90%;}
