/* alert-form style */
.form-wrap{position:relative;background: url(../images/alert01body.png) no-repeat;width: 800px; height: 550px;}
.form-body{position: absolute;left: 50%;margin-left: -400px;top: 50%;margin-top: -273px;}
.form-input{width:370px; height:540px;padding-left:38px;padding-top:30px;}
.form-input p{color:#FFFFFF; font-size:20px;height: 20px; margin-bottom: 10px;}
.form-input span{color: #FFFFFF;font-size:13px;}
.form-input ul{height: 316px;padding-top: 14px;}
.form-input ul li{margin-bottom:14px;}
.form-input ul li input{height: 30px; border: 2px solid #77d9ff; width: 206px;padding-left: 14px;color:#bdbdbd;font-size: 15px;border-radius:2px;}
